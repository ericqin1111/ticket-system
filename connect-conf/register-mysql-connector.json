{
  "name": "order-outbox-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "tasks.max": "1",
    "database.hostname": "mysql-ticket",
    "database.port": "3306",
    "database.user": "root",
    "database.password": "root",
    "database.server.id": "184054",
    "database.server.name": "mysql_server_ticket",
    "topic.prefix": "mysql_server_ticket",
    "database.include.list": "ticket_system",
    "table.include.list": "ticket_system.outbox",
    "schema.history.internal.kafka.bootstrap.servers": "kafka-ticket:9092",
    "schema.history.internal.kafka.topic": "schema-changes.orders",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": "false",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "topic.creation.enable": "true",
    "topic.creation.default.replication.factor": "1",
    "topic.creation.default.partitions": "1",
    "json.output.decimal.format": "numeric"
  }
}